    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@radek-altof/lint-config": "^1.1.1",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
